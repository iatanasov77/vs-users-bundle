<div id="modalUserInfo" class="modal" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">{{ 'vs_users.template.userscrud_userinfo_title' | trans( {},'VSUsersBundle' ) }}</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div id="cardUserInfo" class="card">
                    <div class="card-body"></div>
                </div>
            </div>
            <div class="modal-footer" id="imContainer">
                <button type="button" class="btn btn-primary" id="btnSaveUserInfo">
                    {{ 'vs_application.form.save' | trans( {},'VSApplicationBundle' ) }}
                </button>
                <button type="button" class="btn btn-secondary" data-dismiss="modal">
                    {{ 'vs_application.form.cancel' | trans( {},'VSApplicationBundle' ) }}
                </button>
            </div>
        </div>
    </div>
</div>
