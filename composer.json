{
	"name" : "vankosoft/users-bundle",
	"type" : "symfony-bundle",
	"description" : "Symfony VSUsersBundle",
	"keywords" : [
		"User management"
	],
	"homepage" : "https://github.com/iatanasov77/vs-users-bundle",
	"license" : "MIT",
	"authors" : [{
			"name" : "Ivan I. Atanasov",
			"email" : "i.atanasov77@gmail.com"
		}
	],
	"require" : {
		"php" : "^7.2",
		"vankosoft/application-bundle" : "^1.3",
		"symfony/security-bundle" : "^4.4 || ^5.2",
		"symfony/mailer" : "^4.4 || ^5.2",
		"symfonycasts/verify-email-bundle" : "^1.5",
		"symfonycasts/reset-password-bundle" : "^1.8",
		"lexik/jwt-authentication-bundle" : "^2.11"
	},
	"conflict" : {
		"doctrine/doctrine-bundle" : "<1.3",
		"symfony/doctrine-bridge" : "<2.7"
	},
	"autoload" : {
		"psr-4" : {
			"Vankosoft\\UsersBundle\\" : "lib/"
		}
	},
    "repositories": [
        {
            "type": "path",
            "url": "../*"
        }
    ],
	"minimum-stability" : "dev"
}